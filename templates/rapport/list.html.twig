{% block body %}
    <style>
        .example-wrapper { margin: 1em auto; max-width: 1100px; width: 200%; font: 18px/1.5 sans-serif; }
        .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
        thead {color:indianred;}
        tbody {color:black;}
        tfoot {color:red;}
    </style>

    <h1>Rapport</h1>

    <div class="example-wrapper">

        <table class="table"  >
            <thead>
            <tr>
                <th><div class="form-check" >Titre</div></th>
                <th><div class="form-check">Description</div></th>

            </tr>
            </thead>
            <tbody>
            {% for rapport in rapport %}
                <tr>
                    <td><div class="form-check"> {{ rapport.titre }}</div></td>
                    <td><div class="form-check"> {{ rapport.description }}</div></td>

                </tr>

            {% endfor %}
            </tbody>
        </table>

    </div>

{% endblock %}
